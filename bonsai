#!/usr/bin/env php
<?php
include 'bootstrap/app.php';

use Console\HashCommand;
use Console\RunTestCommand;
use Symfony\Component\Console\Application;


$application = new Application();
$application->add(new HashCommand());
$application->add(new RunTestCommand());
$application->add(new \Console\MakeControllerCommand());
$application->add(new \Console\MakeMiddleWareCommand());
$application->add(new \Console\MakeModelCommand());
$application->add(new \Console\RoutesListCommand('route list ' , $app));
$application->run();
